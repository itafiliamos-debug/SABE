{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "@vercel/php"
    },
    {
      "src": "**/*.{html,css,js,png,jpg,gif,ico,svg,pdf}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "functions": {
    "api/index.php": {
      "maxDuration": 30 
    }
  }
}